     "data": {
      "text/plain": [
       "0.015384615384615384"
      ]
     },
     "execution_count": 32,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "AdvectionLaxFriedrichs_CFL(v_signed,dx)"
   ]
