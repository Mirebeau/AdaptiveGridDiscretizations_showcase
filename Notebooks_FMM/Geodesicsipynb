   "source": [
    "hfmIn = Eikonal.dictIn({\n",
    "    'model':'Isotropic2',\n",
    "    'seed':Cycloid(0.2),\n",
    "    'tip': Cycloid(2.*np.pi-0.2)\n",
    "})\n",
    "dx = 0.4 # Use a slightly larger domain, containing the trajectory in its interior\n",
    "hfmIn.SetRect([[-dx,2.*np.pi+dx],[0,2+dx]],dimx=300)\n",
    "X,Z = hfmIn.Grid()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [],
   "source": [
    "hfmIn['cost'] = 1./np.sqrt(Z)"
